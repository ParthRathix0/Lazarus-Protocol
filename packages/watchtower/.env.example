# Watchtower Configuration
# Copy this to .env and fill in the values

# Private key for the watchtower wallet (without 0x prefix)
WATCHTOWER_PRIVATE_KEY=your_private_key_here

# RPC URLs
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID
DESTINATION_RPC_URL=https://arb1.arbitrum.io/rpc

# Contract Addresses
LAZARUS_SOURCE_ADDRESS=0x...
LAZARUS_VAULT_ADDRESS=0x...
LIFI_DIAMOND_ADDRESS=0x1231DEB6f5749EF6cE6943a275A1D3E7486F4EaE

# Chain IDs
SOURCE_CHAIN_ID=11155111
DESTINATION_CHAIN_ID=42161

# Supported Tokens (format: SYMBOL:ADDRESS,SYMBOL:ADDRESS,...)
SUPPORTED_TOKENS=WETH:0xfFf9976782d46CC05630D1f6eBAb18b2324d6B14,USDC:0x1c7D4B196Cb0C7B01d743Fbc6116a902379C7238

